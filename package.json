{
  "name": "react-live-store",
  "version": "0.1.0",
  "description": "Shared data container for React apps with a mutable-like API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "cross-env npm run cleanup && npm run compile",
    "cleanup": "node -e \"require('fs').rmSync('dist', {force: true, recursive: true});\"",
    "compile": "tsc",
    "prepublishOnly": "npm run build",
    "preversion": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axtk/react-live-store.git"
  },
  "keywords": [
    "react",
    "react store",
    "store",
    "live store"
  ],
  "author": "axtk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/axtk/react-live-store/issues"
  },
  "homepage": "https://github.com/axtk/react-live-store#readme",
  "dependencies": {
    "object-observer": "5.0.0"
  },
  "peerDependencies": {
    "@types/react": ">=16.8",
    "react": ">=16.8"
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "typescript": "4.6.4"
  }
}
